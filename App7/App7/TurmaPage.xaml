<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.TurmaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker1" Title="Turmas"></Picker>
            <Picker x:Name="Picker2" Title="Disciplina"></Picker>
            <Picker x:Name="Picker3" Title="Professores"></Picker>
            <Button x:Name="Button0" Text="Associar Professor"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
            <Button x:Name="Button1" Text="Nova Turma"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked1" />
            <Button x:Name="Button2" Text="Editar Turma"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked2" />
            <Button x:Name="Button3" Text="Voltar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked3" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>