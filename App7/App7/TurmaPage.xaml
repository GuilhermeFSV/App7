<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.TurmaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker" Title="Disciplinas"></Picker>
            <Picker x:Name="Picker2" Title="Turmas"></Picker>
            <Picker x:Name="Picker3" Title="Professores"></Picker>
            <Button x:Name="associarprofessor" Text="Associar Professor"
                   HorizontalOptions="Center"
                    Clicked="ButtonAssociarProfessor"></Button>
            <Button x:Name="novaturma" Text="Nova Turma"
                   HorizontalOptions="Center"
                    Clicked="ButtonNovaTurma"></Button>
            <Button x:Name="editarturma" Text="Editar Turma"
                   HorizontalOptions="Center"
                    Clicked="ButtonEditarTurma"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>