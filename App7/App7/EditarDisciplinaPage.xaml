<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.EditarDisciplinaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker0" Title="Disciplina"
                    SelectedIndexChanged="SelecionarDisciplina">
                </Picker>
                <Entry x:Name="Entry0" Placeholder="Nome da Disciplina" />
                <Button x:Name="Button0" Text="Salvar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
                <Button x:Name="Button1" Text="Excluir"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked1" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>