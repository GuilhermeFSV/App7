<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.NovaDisciplinaPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Entry0" Keyboard="Numeric" Placeholder="Nome" />
            <Entry x:Name="Entry1" Keyboard="Numeric" Placeholder="Horas" />
            <Picker x:Name="Picker" Title="Pré-requisito"></Picker>
            <Button x:Name="Button0" Text="Salvar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>