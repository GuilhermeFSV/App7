<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.ProfessorPage">
    <ContentPage.Content>
        <StackLayout>

            <Picker x:Name="Picker" Title="Professores"></Picker>

            <Button x:Name="butNovo" Text="Novo Professor"
                    HorizontalOptions="Center"
                    Clicked="OnButtonClicked"/>

            <Button x:Name="butEditar" Text="Editar Professor"
                    HorizontalOptions="Center"
                    Clicked="OnButtonClicked1"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>