<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.ProfessorPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker" Title="Professores"></Picker>
            <Button x:Name="Button0" Text="Salvar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
            <Button x:Name="Button1" Text="Novo Professor"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked1" />
            <Button x:Name="Button2" Text="Editar Professor"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked2" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>