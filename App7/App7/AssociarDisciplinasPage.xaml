<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.AssociarDisciplinasPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="PickerListDisciplina" Title="Disciplina">
            </Picker>
            <Picker x:Name="PickerListCursos" Title="Curso">
            </Picker>

            <Button x:Name="ButtonSalvar"
                    Clicked="ButtonSalvar_Clicked"
                    Text="Salvar"
                    HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>