<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.MatricularNoCursoPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker" Title="Matricula"></Picker>

            <Picker x:Name="Picker2" Title="Turma"></Picker>

            <Button x:Name="associar" Text="Associar"
            HorizontalOptions="Center"
            Clicked="ButtonAssociar"/>

            <Button x:Name="remover" Text="Remover"
            HorizontalOptions="Center"
            Clicked="ButtonRemover"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>