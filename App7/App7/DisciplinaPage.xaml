<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.DisciplinaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="PickerDisciplina" Title="Disciplina">
            </Picker>
            <Button x:Name="ButtonNova"
                    Clicked="ButtonNova_Clicked"
                    Text="Nova Disciplina"
                    HorizontalOptions="Center"/>
            <Button x:Name="ButtonEditar"
                    Clicked="ButtonEditar_Clicked"
                    Text="Editar Disciplina"
                    HorizontalOptions="Center"/>
            <Button x:Name="ButtonAssociar"
                    Clicked="ButtonAssociar_Clicked"
                    Text="Associar aos Cursos"
                    HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>