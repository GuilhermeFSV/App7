<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.HistoricoPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker1" Title="Matícula"></Picker>
            <Picker x:Name="Picker2" Title="Histórico"></Picker>
            <Entry x:Name="Entry" Keyboard="Numeric" Placeholder="Faltas" />
            <Picker x:Name="Picker3" Title="Notas"></Picker>
            <Button x:Name="Button0" Text="Lançar Notas"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
            <Button x:Name="Button1" Text="Remover Notas"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked1" />
            <Button x:Name="Button2" Text="Lançar Faltas"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked2" />
            <Button x:Name="Button3" Text="Salvar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked3" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>