        <StackLayout>
            <Picker x:Name="Picker0" Title="Aluno"></Picker>
            <Picker x:Name="Picker1" Title="Curso"></Picker>
            <Button x:Name="Button0" Text="Associar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
