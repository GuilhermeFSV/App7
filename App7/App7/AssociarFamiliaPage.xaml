<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.AssociarFamiliaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker1" Title="Pessoa"></Picker>
            <Entry x:Name="Entry0" Placeholder="Nome" />
            <Entry x:Name="Entry1" Keyboard="Numeric" Placeholder="Periodo" />
            <Entry x:Name="Entry2" Placeholder="Tipo de parentesco" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>