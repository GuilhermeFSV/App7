<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.LançarNotaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker1" Title="Matrícula"></Picker>
            <Picker x:Name="Picker2" Title="Histórico"></Picker>
            <Entry x:Name="Entry1" Keyboard="Numeric" Placeholder="Nota" />
            <Entry x:Name="Entry2" Keyboard="Numeric" Placeholder="Data" />
            <Button x:Name="Button1" Text="Lançar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked1" />

            <Button x:Name="Button2" Text="Cancelar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked2" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>