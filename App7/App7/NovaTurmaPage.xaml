<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.NovaTurmaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker1" Title="Disciplina"></Picker>
            <Picker x:Name="Picker2" Title="Professor"></Picker>
            <Entry x:Name="Entry0" Placeholder="Ano" />
            <Entry x:Name="Entry1" Keyboard="Numeric" Placeholder="Semestre" />
            <Button x:Name="Button0" Text="Salvar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked0" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>